{
    "service": {
        "port": ":6061",
        "root": "data",
        "allowOrigins": [
            "https://account.hatlonely.com",
            "https://tech.hatlonely.com"
        ],
        "cookieSecure": true,
        "cookieDomain": "account.hatlonely.com"
    },
    "account": {
        "address": "go-account:6060",
        "maxConn": 20,
        "connTimeout": "200ms",
        "recvtimeout": "200ms"
    },
    "logger": {
        "infoLog": {
            "filename": "log/cloud.info",
            "maxAge": "24h"
        },
        "warnLog": {
            "filename": "log/cloud.warn",
            "maxAge": "24h"
        },
        "accessLog": {
            "filename": "log/cloud.access",
            "maxAge": "24h"
        }
    }
}